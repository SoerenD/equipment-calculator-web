{
    "name": "dl-equipment-calculator-web",
    "version": "1.0.0",
    "engines": {
        "node": "14.x",
        "npm": "8.x"
    },
    "scripts": {
        "ng": "ng",
        "start": "node dist/dl-equipment-calculator-web/backend/server.js",
        "build": "ng build && npm run build-server",
        "build-server": "cd src/backend && tsc && npm run copy-server && npm run copy-shared",
        "copy-server": "cp -r dist/out-tsc/backend dist/dl-equipment-calculator-web/",
        "copy-shared": "cp -r dist/out-tsc/shared dist/dl-equipment-calculator-web/"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^13.1.2",
        "@angular/common": "^13.1.2",
        "@angular/compiler": "^13.1.2",
        "@angular/core": "^13.1.2",
        "@angular/forms": "^13.1.2",
        "@angular/material": "^13.1.2",
        "@angular/platform-browser": "^13.1.2",
        "@angular/platform-browser-dynamic": "^13.1.2",
        "@angular/router": "^13.1.2",
        "express": "^4.17.2",
        "rxjs": "~7.4.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^13.1.3",
        "@angular/cli": "^13.1.3",
        "@angular/compiler-cli": "^13.1.2",
        "@types/express": "^4.17.13",
        "@types/node": "^12.20.42",
        "eslint": "^8.7.0",
        "prettier": "^2.5.1",
        "ts-node": "^10.4.0",
        "typescript": "~4.5.2"
    },
    "prettier": {
        "printWidth": 120
    }
}
